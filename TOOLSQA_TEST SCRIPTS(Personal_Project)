        city.select_by_visible_text("Jaipur")
        driver.find_element(By.XPATH, "//button[@id='submit']").click()
       except:
           print("Enter valid Email ID")
        driver.close()
    def registerwithoutenteringGender(self):
        driver.get("https://demoqa.com/automation-practice-form")
        driver.maximize_window()
        driver.implicitly_wait(10)
        driver.find_element(By.XPATH, "//input[@id='firstName']").send_keys("Abhishek")
        driver.find_element(By.XPATH, "//input[@id='lastName']").send_keys("A")
        driver.find_element(By.XPATH, "//input[@id='userEmail']").send_keys("akaabhi123@3232.com")
        radiobutton=driver.find_element(By.XPATH, "//label[normalize-space()='Male']")
        radiobutton.is_selected()
        if(radiobutton==True):
            driver.find_element(By.XPATH, "//label[normalize-space()='Male']").click()

        driver.find_element(By.XPATH, "//input[@id='userNumber']").send_keys("9123456789")
        driver.find_element(By.XPATH, "//input[@id='dateOfBirthInput']").click()
        month = Select(driver.find_element(By.XPATH, "//select[@class='react-datepicker__month-select']"))
        month.select_by_visible_text("March")
        year = Select(driver.find_element(By.XPATH, "//select[@class='react-datepicker__year-select']"))
        year.select_by_visible_text("2002")
        driver.find_element(By.XPATH, "//div[contains(@aria-label,'Choose Sunday, March 10th, 2002')]").click()
        driver.execute_script("window.scrollBy(0,3000)", "")
        driver.find_element(By.XPATH, "//label[normalize-space()='Sports']").click()
        driver.find_element(By.XPATH, "//textarea[@id='currentAddress']").send_keys("3, visnathan nagar, Banglore")
        driver.find_element(By.XPATH, "(//div[@id='state']//div[contains(@class,'css-1hwfws3')]").click()
        state = Select(driver.find_element(By.XPATH, "//div[@id='state']//div[contains(@class,'css-1hwfws3')]"))
        state.select_by_visible_text("Rajasthan")
        driver.find_element(By.XPATH, "//div[@id='city']//div[contains(@class,'css-1hwfws3')]").click()
        city = Select(driver.find_element(By.XPATH, "//div[@id='city']//div[contains(@class,'css-1hwfws3')]"))
        city.select_by_visible_text("Jaipur")
        driver.find_element(By.XPATH, "//button[@id='submit']").click()
        driver.close()
    def registerbyDob(self):
        driver.get("https://demoqa.com/automation-practice-form")
