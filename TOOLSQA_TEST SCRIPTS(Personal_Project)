        year.select_by_visible_text("2024")
        driver.find_element(By.XPATH,"//div[contains(@aria-label,'Choose Sunday, March 10th, 2024')]").click()
        driver.execute_script("window.scrollBy(0,3000)","")
        driver.find_element(By.XPATH,"//label[normalize-space()='Sports']").click()
